fastcgi_param   REQUEST_URI             {{nginx_fastcgi_request_uri}}
fastcgi_param   DOCUMENT_URI            {{nginx_fastcgi_document_uri}}
fastcgi_param   DOCUMENT_ROOT           {{nginx_fastcgi_document_root}}
fastcgi_param   SERVER_PROTOCOL         {{nginx_fastcgi_server_protocol}}

fastcgi_param   GATEWAY_INTERFACE       {{nginx_fastcgi_gateway_interface}}
fastcgi_param   SERVER_SOFTWARE         {{nginx_fastcgi_server_software}}

fastcgi_param   REMOTE_ADDR             {{nginx_fastcgi_remote_addr}}
fastcgi_param   REMOTE_PORT             {{nginx_fastcgi_remote_port}}
fastcgi_param   SERVER_ADDR             {{nginx_fastcgi_server_addr}}
fastcgi_param   SERVER_PORT             {{nginx_fastcgi_server_port}}
fastcgi_param   SERVER_NAME             {{nginx_fastcgi_server_name}}

